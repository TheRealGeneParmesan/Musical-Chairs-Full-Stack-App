<div class="-posts container my-4">
  <div class="row my-5 post-list">
    {{#if posts.length}}
      <div class="col-md-6 post-list">
        <h3>Current Posts:</h3>
        {{#each posts}}
          <div class="row mb-2">
            <div class="col-md-8">
              <h4>{{this.title}}</h4>
              <p>{{this.content}}</p>
            </div>
            <div class="col-md-4">
              <button class="btn btn-sm btn-danger" data-id="{{this.id}}">DELETE</button>
            </div>
          </div>
        {{/each}}
      </div>
    {{else}}
      <div class="col-md-12">
        <h5>You currently have no posts</h5>
        <img src="https://media.tenor.com/72TfkDBfeAsAAAAi/gjaja-dj.gif" alt="dj shiba gif" style="">
      </div>
    {{/if}}
  </div>
</div>
<script src="/js/profile.js"></script>